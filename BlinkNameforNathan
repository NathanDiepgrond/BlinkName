const pin_t MY_LED = D7;

SYSTEM_THREAD(ENABLED);

void setup() 
{
   	pinMode(MY_LED, OUTPUT); 
}

void loop() 
{
    //NAME
	//N - Long blink followed by short blink
	digitalWrite(MY_LED, HIGH);
	delay(1s);
	digitalWrite(MY_LED, LOW);
	delay(1s);
	digitalWrite(MY_LED, HIGH);
	delay(250ms);
	digitalWrite(MY_LED, LOW);
	delay(1s);
	
	//A - Short blink followed by long blink
	digitalWrite(MY_LED, HIGH);
	delay(250ms);
	digitalWrite(MY_LED, LOW);
	delay(1s);
	digitalWrite(MY_LED, HIGH);
	delay(1s);
	digitalWrite(MY_LED, LOW);
	delay(1s);
	
	//T - One long blink
	digitalWrite(MY_LED, HIGH);
	delay(1s);
	digitalWrite(MY_LED, LOW);
	delay(1s);
	
	//H - Four short blinks
	digitalWrite(MY_LED, HIGH);
	delay(250ms);
	digitalWrite(MY_LED, LOW);
	delay(1s);
	digitalWrite(MY_LED, HIGH);
	delay(250ms);
	digitalWrite(MY_LED, LOW);
	delay(1s);
	digitalWrite(MY_LED, HIGH);
	delay(250ms);
	digitalWrite(MY_LED, LOW);
	delay(1s);
	digitalWrite(MY_LED, HIGH);
	delay(250ms);
	digitalWrite(MY_LED, LOW);
	delay(1s);
	
	//A - Short blink followed by long blink
	digitalWrite(MY_LED, HIGH);
	delay(250ms);
	digitalWrite(MY_LED, LOW);
	delay(1s);
	digitalWrite(MY_LED, HIGH);
	delay(1s);
	digitalWrite(MY_LED, LOW);
	delay(1s);
	
	//N - Long blink followed by short blink
	digitalWrite(MY_LED, HIGH);
	delay(1s);
	digitalWrite(MY_LED, LOW);
	delay(1s);
	digitalWrite(MY_LED, HIGH);
	delay(250ms);
	digitalWrite(MY_LED, LOW);
	delay(1s);
}
